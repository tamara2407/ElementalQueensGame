package server;

public class Spell {
	
	private String name;
    private int effect;
    private String description;
    private String manaCost;

    public Spell(String name, int effect, String description) {
        this.name = name;
        this.effect = effect;
        this.description=description;
    }

    
    public String getName() {
        return name;
    }

    public int getEffect() {
        return effect;
    }
    
    public String getDescription() {
    	return description;
    }
    
    public String toString() {
        return "Spell{" +
                "name='" + name + '\'' +
                ", effect=" + effect +
                ", manaCost=" + manaCost +
                '}';
    }

}
